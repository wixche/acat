    <Animation name="TopLevelRotation"
               start="true"
               iterations="@GridScanIterations">
      <Widget name="KeyboardBoxTop" onSelect="transition(RowRotationKeyboardBoxTop)"/>
      <Widget name="KeyboardBoxBottom" onSelect="transition(RowRotationKeyboardBoxBottom)"/>
    </Animation>

    <Animation name="RowRotationKeyboardBoxTop"
               iterations="@RowScanIterations"
               firstPauseTime="@FirstPauseTime"
               onEnd="transition(TopLevelRotation)">
      <Widget name="TopRow1" onSelect="transition(ButtonRotationTopRow1)"/>
      <Widget name="TopRow2" onSelect="transition(ButtonRotationTopRow2)"/>
      <Widget name="TopRow3" onSelect="transition(ButtonRotationTopRow3)"/>
      <Widget name="TopRow4" onSelect="transition(ButtonRotationTopRow4)"/>
    </Animation>

