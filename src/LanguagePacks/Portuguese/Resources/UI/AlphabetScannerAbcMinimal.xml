      <Widget name="BottomRow2" onSelect="transition(ButtonRotationBottomRow2)"/>
      <Widget name="BottomRow3" onSelect="transition(ButtonRotationBottomRow3)"/>
      <Widget name="BottomRow4" onSelect="transition(ButtonRotationBottomRow4)"/>
    </Animation>

