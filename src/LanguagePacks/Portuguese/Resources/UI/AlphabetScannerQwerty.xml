      <Widget name="B2"/>
      <Widget name="B3"/>
      <Widget name="B4"/>
      <Widget name="B5" onSelect="showPopup(LetterE, Letra E)"/>
      <Widget name="B6"/>
      <Widget name="B7"/>
    </Animation>

    <Animation name="ButtonRotationRowL2"
